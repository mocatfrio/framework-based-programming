package com.example.json_to_pojo;

public class Book{
    private String coverThumb;
    private String languageCode;
    private String title;
    private Authors[] authors;
    private String description;
    private String subtitle;
    private String[] subjects;
    private String isbn;

    public String getCoverThumb () {
        return coverThumb;
    }

    public void setCoverThumb (String coverThumb) {
        this.coverThumb = coverThumb;
    }

    public String getLanguageCode () {
        return languageCode;
    }

    public void setLanguageCode (String languageCode) {
        this.languageCode = languageCode;
    }
    
    public String getTitle () {
        return Title;
    }

    public void setTitle (String Title) {
        this.Title = Title;
    }
    
    public Authors[] getAuthors () {
        return Authors;
    }

    public void setAuthors (Authors[] Authors) {
        this.Authors = Authors;
    }
    
    public String getDescription () {
        return Description;
    }

    public void setDescription (String Description) {
        this.Description = Description;
    }

    public String getSubtitle () {
        return Subtitle;
    }

    public void setSubtitle (String Subtitle) {
        this.Subtitle = Subtitle;
    }
    

    public String[] getSubjects () {
        return Subjects;
    }

    public void setSubjects (String[] Subjects) {
        this.Subjects = Subjects;
    }

    public String getISBN () {
        return ISBN;
    }

    public void setISBN (String ISBN) {
        this.ISBN = ISBN;
    }

    @Override
    public String toString()
    {
        return "ClassPojo [CoverThumb = "+CoverThumb+", LanguageCode = "+LanguageCode+", Subtitle = "+Subtitle+", Description = "+Description+", ISBN = "+ISBN+", Authors = "+Authors+", Title = "+Title+", Subjects = "+Subjects+"]";
    }
}