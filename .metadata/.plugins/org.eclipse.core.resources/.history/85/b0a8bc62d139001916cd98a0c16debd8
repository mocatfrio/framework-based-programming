package com.example.json_to_pojo;

import com.google.gson.annotations.SerializedName;

public class Authors {
	@SerializedName("Name")
	private String name;
	
	 public String getName () {
        return name;
    }

    public void setName (String name) {
        this.name = name;
    }
    
    @Override
    public String toString() {
    	return "Deskripsi buku yang dicari:\n" + 
    			"\n\t Title = "+ title + 
    			"\n\t Authors = " + authors +
    			"\n\t ISBN = "+ isbn +
    			"\n\t Description = "+ description + 
    			"\n\t Subtitle = "+ subtitle + 
    			"\n\t Subjects = " + subjects + 
    			"\n\t Language Code = " + languageCode + 
    			"\n\t Cover Thumb = " + coverThumb + "\n";
    }
}
